<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@color/bg_register"
    tools:context="com.example.saravanan.sample.Main6Activity">
    <ScrollView
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:id="@+id/scrollView">




        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/textView31"
            android:layout_marginTop="0dp"
            android:layout_alignParentEnd="true"
            android:orientation="vertical">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="SEMESTER 5"
                android:id="@+id/textView31"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:textColor="@color/white"
                android:textStyle="bold"
                android:layout_gravity="center_horizontal" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="DISCRETE MATHEMATICS"
                android:id="@+id/textView61"
                android:layout_marginTop="44dp"
                android:layout_below="@+id/textView4"
                android:textColor="@color/white"

                android:layout_alignParentStart="true" />

            <Spinner
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:id="@+id/spinner01"
                android:layout_alignTop="@+id/textView61"
                android:layout_toEndOf="@+id/textView31"
                android:textAlignment="center" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="INTERNET PROGRAMMING"
                android:id="@+id/textView7"
                android:layout_alignParentStart="true"
                android:textColor="@color/white"

                android:layout_below="@+id/spinner01" />

            <Spinner
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:id="@+id/spinner21"
                android:layout_centerVertical="true"
                android:layout_alignStart="@+id/spinner01"
                android:textAlignment="center" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="OBJECT ORIENTED ANLYSIS AND DESIGN"
                android:id="@+id/textView81"
                android:layout_below="@+id/spinner21"
                android:textColor="@color/white"

                android:layout_alignEnd="@+id/textView61" />

            <Spinner
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:id="@+id/spinner31"
                android:layout_alignTop="@+id/textView81"
                android:layout_alignStart="@+id/spinner21"
                android:textAlignment="center" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="THEORY OF COMPUTATION"
                android:id="@+id/textView9"
                android:layout_below="@+id/spinner31"
                android:textColor="@color/white"

                android:layout_alignParentStart="true" />

            <Spinner
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:id="@+id/spinner41"
                android:layout_below="@+id/spinner31"
                android:layout_alignStart="@+id/spinner31"
                android:textAlignment="center" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="COMPUTER GRAPHICS"
                android:id="@+id/textView10"
                android:layout_below="@+id/spinner41"
                android:textColor="@color/white"

                android:layout_alignParentStart="true" />

            <Spinner
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:id="@+id/spinner51"
                android:layout_below="@+id/spinner41"
                android:layout_alignStart="@+id/spinner41"
                android:textAlignment="center" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="CASE TOOLS LABORATORY"
                android:id="@+id/textView11"
                android:layout_below="@+id/spinner51"
                android:textColor="@color/white"

                android:layout_alignParentStart="true" />

            <Spinner
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:id="@+id/spinner61"
                android:layout_below="@+id/spinner51"
                android:layout_alignStart="@+id/spinner51"
                android:textAlignment="center" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="INTERNET PROGRAMMING LABORATORY"
                android:id="@+id/textView12"
                android:layout_below="@+id/spinner61"
                android:textColor="@color/white"

                android:layout_alignParentStart="true" />

            <Spinner
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:id="@+id/spinner71"
                android:layout_below="@+id/spinner61"
                android:layout_alignStart="@+id/spinner61"
                android:textAlignment="center" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="COMPUTER GRAPHICS LABORATORY"
                android:id="@+id/textView13"
                android:layout_below="@+id/spinner71"
                android:textColor="@color/white"

                android:layout_alignParentStart="true" />

            <Spinner
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:id="@+id/spinner81"
                android:layout_below="@+id/spinner71"
                android:layout_alignStart="@+id/spinner71"
                android:textAlignment="center" />


            <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:tools="http://schemas.android.com/tools"
                android:layout_width="fill_parent"
                android:layout_height="match_parent"
                android:paddingBottom="@dimen/activity_vertical_margin"
                android:paddingLeft="@dimen/activity_horizontal_margin"
                android:paddingRight="@dimen/activity_horizontal_margin"
                android:paddingTop="@dimen/activity_vertical_margin"
                tools:context="com.example.saravanan.sample.MainActivity"
                android:gravity="center">


                <Button
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="SUBMIT"
                    android:id="@+id/button1"
                    android:layout_below="@+id/spinner81"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"
                    android:background="#ea4c88"
                    android:textColor="@color/white"
                    android:padding="10dp"/>
            </RelativeLayout>

        </LinearLayout>
    </ScrollView>



</RelativeLayout>
